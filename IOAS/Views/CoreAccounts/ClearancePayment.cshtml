                }
            } else if (currentTab == 3) {

                var validationMsg = ValidateTransaction()
                if (validationMsg != 'valid') {
                    hadError = true;
                    $("#alert").text(validationMsg);
                    $('#Validation').modal('show');
                }
            }else if (currentTab == 4) {
                if ($('#CheckListVerified_By').val() == '') {
                    hadError = true;
