                                        {
                                            var count = 0;
                                            foreach (var item in Model.PCFCreditDetails)
                                            {
                                                var projectid = "PCFCreditDetails[" + count + "].ProjectId";
                                                var piid = "PCFCreditDetails[" + count + "].PIId";
                                                var selid = "PCFCreditDetails[" + count + "].Id";
                                                var bankid = "PCFCreditDetails[" + count + "].BankId";
