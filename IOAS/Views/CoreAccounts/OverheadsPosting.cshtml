                                                var receiptnumber = "PCFCreditDetails[" + count + "].ReceiptNumber";
                                                var pcfamt = "PCFCreditDetails[" + count + "].Amount";
                                                var pcfpercent = "PCFCreditDetails[" + count + "].Percent";

