                    <div class="row">
                        <div class="col-md-12">
                            <div class="row m-b-sm">
                                <div class="col-md-6">
                                    <h3 class="mt-xs">Details</h3>
                                </div>
                                @*<div class="col-md-6 text-right">
                                        <a href="javascript:void(0)" id="btnAddHonor" class="btn btn-primary btnAddHonor">Add New</a>
                                    </div>*@
                            </div>
