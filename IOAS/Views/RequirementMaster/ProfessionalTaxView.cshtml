@model IOAS.Models.ProfessionalTaxModel
@{
    ViewBag.Title = "ProfessionalTaxView";
    Layout = "~/Views/Shared/_LayoutFluid.cshtml";
}
<div class="row hd-tt-bg">
    <div class="col-md-12">
        <h4 class="pull-left"><b>Professional Tax View</b></h4>
    </div>
</div>
<div class="vw-dts-bg">
    <div class="row">
        <div class="col-md-12">
            <table class="table table-bordered">
                <thead>
                </thead>
                <tbody>
                    <tr>
                        <td>
                            <label>Monthy Salary Rs. 3,500/</label>
                            <h4>@Html.DisplayFor(m => m.MonthySalary35)</h4>
                        </td>
                        <td>
                            <label>Monthy Salary Rs. 3,501 - Rs. 5,000</label>
                            <h4>@Html.DisplayFor(m => m.MonthySalary35to5)</h4>
                        </td>
                        <td>
                            <label>Monthy Salary Rs. 5,001 - Rs. 7,500</label>
                            <h4>@Html.DisplayFor(m => m.MonthySalary5to75)</h4>
                        </td>
                        <td>
                            <label>Monthy Salary Rs. 7,501 - Rs. 10,000</label>
                            <h4>@Html.DisplayFor(m => m.MonthySalary75to10)</h4>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label>Monthy Salary Rs. 10,001 - Rs. 12,500</label>
                            <h4>@Html.DisplayFor(m => m.MonthySalary10to12)</h4>
                        </td>
                        <td>
                            <label>Above - Rs. 12,501</label>
                            <h4>@Html.DisplayFor(m => m.MonthySalaryAbove12)</h4>
                        </td>
                        <td>
                            <label>Value Date </label>
                            <h4>@Html.DisplayFor(m => m.ValueDateType)</h4>
                        </td>
                        <td>
                            <label>End Date </label>
                            <h4>@Html.DisplayFor(m => m.EndDateType)</h4>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12 mb-lg text-center">
        <input type="button" onclick="window.location.href='@Url.Action("ProfessionalTaxList", "RequirementMaster")';" class="btn btn-default" value="Close" id="btnclose" />
    </div>
</div>
