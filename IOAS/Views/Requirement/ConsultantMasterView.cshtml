
@model IOAS.Models.ConsultantMasterView
@{
    ViewBag.Title = "ConsultantMasterView";
    Layout = "~/Views/Shared/_LayoutFluid.cshtml";
}

<style>
    .vw-dts-bg .hgt-bg {
        background: #597df0;
        color: #fff;
    }

        .vw-dts-bg .hgt-bg label {
            font-size: 16px !important;
            color: #fff !important;
        }

        .vw-dts-bg .hgt-bg h4 {
            font-size: 18px !important;
            color: #fff;
        }

    .vw-dts-bg .hgt-bg-pl {
        background: #e11eda;
        color: #fff;
    }

        .vw-dts-bg .hgt-bg-pl label {
            font-size: 16px !important;
            color: #fff !important;
        }

        .vw-dts-bg .hgt-bg-pl h4 {
            font-size: 18px !important;
            color: #fff;
        }
</style>

<div class="row hd-tt-bg">
    <div class="col-md-12">
        <h4><b>Consultant Master</b> </h4>
    </div>
</div>

<div class="row">
    <div class="white-bg ad-navbar">
        <div class="col-md-4 ds-spl-bg">
            <div class="ds-icn">
                <i class="ion-ios-list-outline "></i>
            </div>
            <div class="ds-tt">
                <label>Consultant Name :</label>
                @Html.DisplayFor(m => m.ConsultantName)
            </div>
        </div>
        
    </div>
</div>



<div class="row vw-dts-bg">
    <div class="col-md-12">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th colspan="4">Personal Details:</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        <label>Nationality :</label>
                        @Html.DisplayFor(m => m.Nationality)
                    </td>
                    <td>
                        <label>Category :</label>
                        @Html.DisplayFor(m => m.Category)
                    </td>
                    <td>
                        <label>Address :</label>
                        @Html.DisplayFor(m => m.Address)
                    </td>
                    <td>
                        <label>Email :</label>
                        @Html.DisplayFor(m => m.Email)
                    </td>
                </tr>
                <tr>
                    <td>
                        <label>Phone Number :</label>
                        @Html.DisplayFor(m => m.MobileNo)

                    </td>
                    <td>
                        <label>Pan Number :</label>
                        @Html.DisplayFor(m => m.PAN)

                    </td>
                    <td>
                        <label>Aadhaar Number :</label>
                        @Html.DisplayFor(m => m.AadhaarNo)

                    </td>
                    <td>
                        @if (!string.IsNullOrEmpty(Model.TIN))
                        {
                            <label>TIN Number :</label>
                            @Html.DisplayFor(m => m.TIN)
                        }
                        else
                        {
                            <label>GSTIN Number :</label>
                            @Html.DisplayFor(m => m.GSTIN)
                        }
                    </td>

                </tr>
                <tr>
                    <td>
                        <label>State :</label>
                        @Html.DisplayFor(m => m.State)

                    </td>
                    <td>
                        <label>State Code :</label>
                        @Html.DisplayFor(m => m.StateCode)

                    </td>
                    <td>
                        <label>City :</label>
                        @Html.DisplayFor(m => m.City)

                    </td>
                    <td>
                        <label>Pin Code :</label>
                        @Html.DisplayFor(m => m.PinCode)

                    </td>
                </tr>
            </tbody>
        </table>
    </div>

    <div class="col-md-12">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th colspan="4">Consultant Bank Details</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        <label>Account Holder's Name : </label>
                        @Html.DisplayFor(m => m.AccHolderName)
                    </td>
                    <td>
                        <label>Bank Name :</label>
                        @Html.DisplayFor(m => m.BankName)
                    </td>
                    <td>
                        <label>Branch :</label>
                        @Html.DisplayFor(m => m.Branch)
                    </td>
                    <td>
                        <label>IFSC :</label>
                        @Html.DisplayFor(m => m.IFSC)
                    </td>
                </tr>
                <tr>
                    <td>
                        <label>Account Number : </label>
                        @Html.DisplayFor(m => m.AccNo)
                    </td>
                    <td>
                        <label>Bank Address:</label>
                        @Html.DisplayFor(m => m.BankAddress)
                    </td>
                    <td>
                        <label>Bank Nature:</label>
                        @Html.DisplayFor(m => m.BankNature)
                    </td>
                    <td>
                        <label>Bank Email:</label>
                        @Html.DisplayFor(m => m.BankEmail)
                    </td>
                </tr>
                <tr>
                    <td>
                        <label>ABA Number : </label>
                        @Html.DisplayFor(m => m.ABANumber)
                    </td>
                    <td>
                        <label>Sort Code : </label>
                        @Html.DisplayFor(m => m.SortCode)
                    </td>
                    <td>
                        <label>IBAN : </label>
                        @Html.DisplayFor(m => m.IBAN)
                    </td>
                    <td>
                        <label>MICR Code : </label>
                        @Html.DisplayFor(m => m.MICRCode)
                    </td>

                </tr>
                <tr>
                    <td>
                        <label>SWIFT / BIC code :</label>
                        @Html.DisplayFor(m => m.SWiftCode)
                    </td>
                    <td>
                        <label>Bank Country :</label>
                        @Html.DisplayFor(m => m.BankCountry)
                    </td>
                </tr>
            </tbody>
        </table>
    </div>        
    <div class="col-md-12">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th colspan="4">Other Document</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    
                    <td class="text-center"><b>Attachment Name</b></td>
                    <td class="text-center"><b>Attachment</b></td>
                </tr>
                @if (Model.TdsDocument != null)
                {
                    foreach (var item in Model.TdsDocument)
                    {
                        <tr>
                            
                            <td class="text-center">
                                @Html.DisplayFor(m => item.TdsAttachementName)
                            </td>
                            <td class="text-center">
                                <a href="/Account/ShowDocument?file=@item.TdsDocumentPath&filepath=~%2FContent%2Requirement%2F" target="_blank">@item.TdsDocumentName</a>
                            </td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
   
    <div class="col-md-offset-6 col-md-12">

        <input type="button" onclick="window.location.href='@Url.Action("ConsultantMaster", "Requirement")';" class="btn btn-primary" value="Close" />
    </div>
</div>

<input type="hidden" id="currentRefId" value="@Model.Consultant_MasterId" />
<input type="hidden" id="processGuideLineId" value="@ViewBag.processGuideLineId" />


